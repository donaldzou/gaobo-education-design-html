<script>
    $('.WeChat').mouseover(function(){
        $(this).children().last().show()
    })

    $('.WeChat').mouseleave(function(){
        $(this).children().last().hide()
    })

    var activity = $.cookie('activity');
    if( activity ){
        $('.activity').hide();
    }
    $('.activity .close').click(function(){
        $.cookie('activity', 1,{ path:'/'});
        $(this).parent().remove();
    })

    var window_w = $(window).width();
    var right = ( window_w - 1030 ) / 2 ;
    var slider_w = $('.slider').css('width')
    $('.slider').css('right',right - parseInt(slider_w) +'px')
    $('.activity').css('right',right - parseInt(slider_w) +'px')


    var i = 0;
    $(window).resize(function(){
        var window_w = $(window).width();
        var right = ( window_w - 1000 ) / 2 ;
        var slider_w = $('.slider').css('width')
        $('.slider').css('right',right - parseInt(slider_w) +'px')
        $('.activity').css('right',right - parseInt(slider_w) +'px')
    })

    function ToTop(){
        $('body,html').animate({
            scrollTop:0
        },'fast','swing')
    }

    function refresh(obj){
        var url = $(obj).attr('_src');
        $(obj).attr('src',url+'?t='+Math.random(100000,999999))
        $(obj).css('backgroundImage','url('+url+'?t='+Math.random(100000,999999)+')')
    }

    $(document).on('click','.code',(function(){
        $(this).attr('src',$(this).attr('src')+'?t='+Math.random());
    }))
</script>

